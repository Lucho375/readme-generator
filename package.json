{
  "name": "readme-writter",
  "version": "1.1.2",
  "description": "CLI readme generator",
  "type": "module",
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "format": "prettier --write .",
    "lint": "eslint .",
    "dev": "node --loader ts-node/esm index.ts",
    "publish": "npm publish",
    "prepublish": "npm run clean && npm run build"
  },
  "bin": {
    "readme-writter": "./dist/index.js"
  },
  "keywords": [
    "readme generator",
    "readme",
    "generator",
    "cli",
    "inquirer",
    "typescript"
  ],
  "author": {
    "name": "Luciano Rodriguez"
  },
  "license": "ISC",
  "dependencies": {
    "inquirer": "9.2.15"
  },
  "devDependencies": {
    "@types/inquirer": "9.0.7",
    "@types/node": "20.11.20",
    "@types/typescript": "2.0.0",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "eslint": "8.57.0",
    "eslint-config-standard-with-typescript": "43.0.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-n": "16.6.2",
    "eslint-plugin-promise": "6.1.1",
    "prettier": "3.2.5",
    "ts-node": "10.9.2",
    "typescript": "5.3.3"
  },
  "repository": {
    "url": "https://github.com/Lucho375/readme-generator.git",
    "type": "git"
  }
}
